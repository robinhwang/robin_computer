[user]
	name = robinhwang
	email = binbinhuangcsu@gmail.com
[credential]
	helper = cache --timeout=3600
[color]
	ui = true
[core]
	editor = emacs
[alias]
	ci = commit -a -v
	a  = add .
	co = checkout
	br = branch
	p  = push
	st = status
	t  = reset --hard HEAD
	tl = reset --hard HEAD^
  l  = log
  ra = remote add
  rr = remote rm
  pu = pull
  cl = clone
  d  = diff
  dc = diff --cached
  aa = commit -C HEAD -a --amend
[merge]
	tool = kdiff3
